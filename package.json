  ],
  "bugs": {
    "url": "https://github.com/compute-io/manhattan-distance/issues"
  },
  "dependencies": {
    "validate.io-array": "^1.0.5",
    "validate.io-function": "^1.0.2"
  },
  "devDependencies": {
    "chai": "2.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "^1.0.0"
  },
